@model Domain.Relationship

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Relationship</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.RelationshipType)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RelationshipType.Type)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Child)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Child.FirstName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Parent)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Parent.FirstName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.RelationshipId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
